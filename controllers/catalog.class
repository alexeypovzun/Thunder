<?php
class Product {

	public function __construct($id,$code,$name,$price,$stock,$description,$image,$slug) {
    	$this->id = $id;
    	$this->code = $code;
    	$this->name = $name;
    	$this->price = $price;
    	$this->stock = $stock;
    	$this->description = $description;
    	$this->image = $image;
    	$this->slug = $slug;
    }

    public function Draw() {
		include('views/catalog.tpl');
	}
}

?>